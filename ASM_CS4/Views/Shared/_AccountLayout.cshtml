<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewBag.Title</title>
    @* <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" /> *@
    <link rel="stylesheet" href="~/LayoutUser/css/bootstrap.min.css" type="text/css" />
    <link rel="stylesheet" href="~/LayoutUser/css/font-awesome.min.css" type="text/css" />
    <link rel="stylesheet" href="~/LayoutUser/css/elegant-icons.css" type="text/css" />
    <link rel="stylesheet" href="~/LayoutUser/css/nice-select.css" type="text/css" />
    <link rel="stylesheet" href="~/LayoutUser/css/jquery-ui.min.css" type="text/css" />
    <link rel="stylesheet" href="~/LayoutUser/css/owl.carousel.min.css" type="text/css" />
    <link rel="stylesheet" href="~/LayoutUser/css/slicknav.min.css" type="text/css" />
    <link rel="stylesheet" href="~/LayoutUser/css/account.css" asp-append-version="true" />
</head>
<body>

    <div>
        @RenderBody()
    </div>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
